<h1>Courses#index</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Credit</th>
    <th colspan="3"></th>
  </tr>

  <% @courses.each do |course| %>
      <tr>
        <td><%= course.title %></td>
        <td><%= course.text %></td>
        <td><%= link_to 'Show', course_path(course) %></td>
        <td><%= link_to 'Edit', edit_course_path(course) %></td>
      </tr>
  <% end %>
</table>
